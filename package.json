{
  "name": "svelvg",
  "version": "0.5.0",
  "license": "MIT",
  "description": "Convert SVG files into Svelte components with TypeScript definitions",
  "author": "Eric Liu (https://github.com/metonym)",
  "main": "./dist/svelvg.umd.js",
  "module": "./dist/svelvg.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/svelvg.es.js",
      "require": "./dist/svelvg.umd.js"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "prepack": "vite build && tsc"
  },
  "dependencies": {
    "svelte": "^3.42.4",
    "tiny-glob": "^0.2.9"
  },
  "devDependencies": {
    "@types/node": "^16.7.10",
    "typescript": "^4.4.2",
    "vite": "^2.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metonym/svelvg"
  },
  "homepage": "https://github.com/metonym/svelvg",
  "bugs": "https://github.com/metonym/svelvg/issues",
  "keywords": [
    "svelte",
    "svg"
  ],
  "files": [
    "dist",
    "cli.js"
  ],
  "bin": {
    "svelvg": "./cli.js"
  }
}
